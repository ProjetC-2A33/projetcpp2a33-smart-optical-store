     <property name="styleSheet">
      <string notr="true">/* --- SPINBOX MODERNE AVEC FLÈCHES --- */
QSpinBox {
    background-color: #ffffff;
    color: #1e1e2f;
    border: 1.5px solid #e0e6ed;
    border-radius: 12px;
    padding-right: 28px;                    /* espace réservé aux flèches */
    padding-left: 10px;
    font-family: &quot;Segoe UI&quot;;
    font-size: 14px;
    selection-background-color: #cfe3ff;
    selection-color: #1e1e2f;
    transition: all 0.25s ease;
    box-shadow: 0 2px 5px rgba(0,0,0,0.05);
}

/* --- SURVOL --- */
QSpinBox:hover {
    background-color: #f4f7ff;
    border-color: #c0d3ff;
    box-shadow: 0 4px 10px rgba(0,0,0,0.08);
}

/* --- FOCUS (clic dans la zone) --- */
QSpinBox:focus {
    border-color: #7aa2ff;
    box-shadow: 0 0 0 3px rgba(122,162,255,0.2);
}

/* --- BOUTONS DE CONTRÔLE (FLÈCHES) --- */
QSpinBox::up-button, QSpinBox::down-button {
    subcontrol-origin: border;
    background-color: transparent;
    border: none;
    width: 22px;
    margin: 2px;
    border-radius: 6px;
}

/* --- Position des boutons --- */
QSpinBox::up-button {
    subcontrol-position: top right;
}
QSpinBox::down-button {
    subcontrol-position: bottom right;
}

/* --- Flèche HAUT --- */
QSpinBox::up-arrow {
    image: url(:/qt-project.org/styles/commonstyle/images/arrowup.png);
    width: 10px;
    height: 10px;
}

/* --- Flèche BAS --- */
QSpinBox::down-arrow {
    image: url(:/qt-project.org/styles/commonstyle/images/arrowdown.png);
    width: 10px;
    height: 10px;
}

/* --- Hover sur les flèches --- */
QSpinBox::up-button:hover,
QSpinBox::down-button:hover {
    background-color: #e8f0ff;
}

/* --- Pressed --- */
QSpinBox::up-button:pressed,
QSpinBox::down-button:pressed {
    background-color: #dbe7ff;
}
</string>
